CREATE TABLE NoteEntity (
  id INTEGER PRIMARY KEY NOT NULL,
  title TEXT NOT NULL,
  content TEXT NOT NULL,
  timeStamp INTEGER NOT NULL,
  color INTEGER NOT NULL
);

getAllNotes:
    SELECT *
    FROM NoteEntity;

getNoteById:
    SELECT *
    FROM NoteEntity
    WHERE id = :id;

insertNote:
    INSERT OR REPLACE
    INTO NoteEntity
    VALUES (?,?,?,?,?);

deleteNote:
    DELETE FROM NoteEntity
    WHERE id = :id;

